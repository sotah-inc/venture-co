{
  "name": "@sotah-inc/api",
  "version": "2.0.66",
  "description": "SotAH API SDK",
  "main": "build/dist/index.js",
  "typings": "build/dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "debug": "npm run -s build:watch",
    "start": "node .",
    "lint": "tslint ./src/**/*.ts{,x}",
    "lint:watch": "chokidar --silent ./src/**/*.ts{,x} -c \"npm run -s lint\"",
    "build": "run-s build:ts",
    "build:watch": "run-p build:ts:watch lint:watch",
    "build:ts": "tsc",
    "build:ts:watch": "npm run -s build:ts -- --watch",
    "prepublishOnly": "npm run -s build",
    "test": "ava -s --fail-fast ./build/dist/test/**/*.js",
    "test:watch": "npm run -s test -- -w",
    "test-coverage": "nyc --reporter=lcov npm -s test",
    "test-coverage-report": "nyc report",
    "test-coverage-check": "nyc check-coverage --lines 95 --functions 75 --branches 95",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "ar-login": "artifactregistry-auth ./.npmrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sotah-inc/venture-co.git"
  },
  "author": "Adrian Parker <adrian@sotah.info>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sotah-inc/venture-co/issues"
  },
  "homepage": "https://github.com/sotah-inc/venture-co#readme",
  "devDependencies": {
    "@blueprintjs/tslint-config": "^3.0.3",
    "ava": "^3.13.0",
    "chokidar-cli": "^2.1.0",
    "coveralls": "^3.1.0",
    "google-artifactregistry-auth": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "tslint": "^6.1.3",
    "typescript": "4.0.5",
    "uuid": "^8.3.1"
  },
  "dependencies": {
    "@google-cloud/error-reporting": "^2.0.0",
    "@google-cloud/firestore": "^4.7.1",
    "@google-cloud/logging-winston": "^4.0.1",
    "@google-cloud/pubsub": "^2.6.0",
    "@google-cloud/storage": "^5.5.0",
    "@sendgrid/mail": "^7.4.0",
    "@sotah-inc/core": "^2.0.138",
    "@sotah-inc/server": "^2.0.29",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.8",
    "@types/fs-extra": "^9.0.4",
    "@types/http-status": "^1.1.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/markdown-it": "10.0.3",
    "@types/nunjucks": "^3.1.3",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/qs": "^6.9.5",
    "@types/speakingurl": "^13.0.2",
    "@types/stripe": "^7.13.25",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.9",
    "async-middleware": "^1.2.1",
    "bcrypt": "^5.0.0",
    "bollinger-bands": "^3.0.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "http-status": "^1.4.2",
