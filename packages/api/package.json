{
  "name": "@sotah-inc/api",
  "version": "1.0.7",
  "description": "SotAH API SDK",
  "main": "build/dist/index.js",
  "typings": "build/dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "debug": "run-p build:watch lint:watch",
    "start": "node .",
    "lint": "tslint ./src/**/*.ts{,x}",
    "lint:watch": "chokidar --silent ./src/**/*.ts{,x} -c \"npm run -s lint\"",
    "build": "tsc",
    "build:clean": "run-s clean build",
    "build:watch": "npm run -s build -- --watch",
    "prepublish": "npm run -s build:clean",
    "test": "ava -s ./build/dist/test/**/*.js",
    "test:watch": "npm run -s test -- -w",
    "test-coverage": "nyc --reporter=lcov npm -s test",
    "test-coverage-report": "nyc report",
    "test-coverage-check": "nyc check-coverage --lines 95 --functions 75 --branches 95",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ihsw/sotah-api.git"
  },
  "author": "Adrian Parker <adrian@sotah.info>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ihsw/sotah-api/issues"
  },
  "homepage": "https://github.com/ihsw/sotah-api#readme",
  "devDependencies": {
    "@blueprintjs/tslint-config": "^1.8.0",
    "ava": "^2.0.0",
    "chokidar-cli": "^1.2.2",
    "coveralls": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1",
    "uuid": "^3.3.2"
  },
  "dependencies": {
    "@google-cloud/error-reporting": "^1.0.0",
    "@google-cloud/firestore": "^2.2.0",
    "@google-cloud/logging-winston": "^0.11.1",
    "@google-cloud/pubsub": "^0.29.1",
    "@google-cloud/storage": "^2.5.0",
    "@sendgrid/mail": "^6.4.0",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^7.0.0",
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/markdown-it": "0.0.7",
    "@types/nunjucks": "^3.1.1",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/speakingurl": "^13.0.2",
    "@types/stripe": "^6.26.7",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "@types/yup": "^0.26.14",
    "async-middleware": "^1.2.1",
    "bcrypt": "^3.0.6",
    "bollinger-bands": "^3.0.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "fs-extra": "^8.0.1",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^8.4.2",
    "moment": "^2.24.0",
    "moving-averages": "^4.0.5",
    "nats": "^1.3.0",
    "nodemon": "^1.19.1",
    "npm": "^6.9.0",
    "nunjucks": "^3.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.13",
    "speakingurl": "^14.0.1",
    "stripe": "^7.1.0",
    "typeorm": "^0.2.18",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "yup": "^0.27.0"
  },
  "nyc": {
    "exclude": []
  },
  "publishConfig": {
    "access": "public"
  }
}
