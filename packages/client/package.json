  "description": "SotAH Web Client SDK",
  "main": "./build/dist/index.js",
  "typings": "build/dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "debug": "run-p build:ts:watch lint:watch build:styles:watch",
    "start": "node .",
    "lint": "tslint ./src/**/*.ts{,x}",
    "lint:watch": "chokidar --silent ./src/**/*.ts{,x} -c \"npm run -s lint\"",
    "build": "run-s build:ts build:styles",
    "build:watch": "run-p build:ts:watch build:styles:watch",
    "build:ts": "tsc",
    "build:ts:watch": "npm run -s build:ts -- --watch",
    "build:styles": "gulp -f ./build/dist/gulpfile.js --cwd . sass",
    "build:styles:watch": "gulp -f ./build/dist/gulpfile.js --cwd . sass:watch",
    "prepare": "npm run -s build"
  },
  "repository": {
